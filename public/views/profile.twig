<!-- ***** Contact Area Start ***** -->
<div class="dorne-contact-area d-md-flex" id="contact">
    <!-- Contact Form Area -->
    <div class="contact-form-area equal-height">

        <div class="contact-form">
            <div class="contact-form-title">
                <h6>Owner Details</h6>
            </div>
            <form action="/v1/room/add" method="post">
                <div class="row">
                    <div class="col-12 col-md-6">
                        <input type="text" name="room_name" class="form-control" placeholder="Owner Name">
                    </div>

                    <select class="custom-select" id="destinations">
                        <option selected>Gender</option>
                        <option value="1">Male</option>
                        <option value="2">Female</option>
                    </select>

                    <div class="col-12 col-md-6">
                        <input type="text" name="room_type" class="form-control" placeholder="Email">
                    </div>
                    <div class="col-12">
                        <textarea name="description" class="form-control" id="Description" cols="30" rows="10" placeholder="Short Description"></textarea>
                    </div>
                    <div class="col-12 col-md-6">
                        <input type="text" name="occupancy" class="form-control" placeholder="Contact Number ">
                    </div>


                    <br/>
                    <div class="col-12">
                        <button type="submit" class="btn dorne-btn">UPDATE</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
    {#<!-- Map Area -->#}
    {#<div class="dorne-map-area equal-height">#}
    {#<div id="googleMap"></div>#}
    {#</div>#}
