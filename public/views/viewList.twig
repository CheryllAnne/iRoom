<!DOCTYPE html>
<html lang="en">


<head>
    <meta charset="UTF-8">
    <meta name="description" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    {#<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>#}
    <!-- The above 4 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <link rel="stylesheet" type="text/css" href="/res/css/bootstrap/bootstrap.min.css">
    <!-- Title -->
    <title>iRooms - Find Comfort!</title>

    <!-- Favicon -->
    <link rel="icon" href="/res/img/core-img/favicon.ico">

    <!-- Core Stylesheet -->
    <link href="/style.css" rel="stylesheet">

    <!-- Responsive CSS -->
    <link rel="stylesheet" type="text/css" href="/res/css/responsive/responsive.css" >

</head>

<body>

<!-- ***** Search Form Area ***** -->
<div class="dorne-search-form d-flex align-items-center">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="search-close-btn" id="closeBtn">
                    <i class="pe-7s-close-circle" aria-hidden="true"></i>
                </div>
                <form action="/v1/room/search" method="get">
                    <input type="search" name="caviarSearch" id="search" placeholder="Search Your Desire Destination">
                    <input type="submit" class="d-none" value="submit">
                </form>
            </div>
        </div>
    </div>
</div>

<!-- ***** Header Area Start ***** -->
<header class="header_area" id="header">
    <div class="container-fluid h-100">
        <div class="row h-100">
            <div class="col-12 h-100">
                <nav class="h-100 navbar navbar-expand-lg">
                    {#<a class="navbar-brand" href="/"><img src="/res/img/core-img/logo.png" alt=""></a>#}
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#dorneNav" aria-controls="dorneNav" aria-expanded="false" aria-label="Toggle navigation"><span class="fa fa-bars"></span></button>
                    <!-- Nav -->
                    <div class="collapse navbar-collapse" id="dorneNav">
                        <ul class="navbar-nav mr-auto" id="dorneMenu">
                            <li class="nav-item active">
                                <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Explore <i class="fa fa-angle-down" aria-hidden="true"></i></a>
                                <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                    <a class="dropdown-item" href="/">Home</a>
                                    <a class="dropdown-item" href="explore.twig">Explore</a>
                                    <a class="dropdown-item" href="/v1/room/view">Listing</a>
                                    {#<a class="dropdown-item" href="single-listing.twig">Single Listing</a>#}
                                    {#<a class="dropdown-item" href="addListing.twig">Contact</a>#}
                                </div>
                            </li>
                            <li class="nav-item ">
                                <a class="nav-link " href="/v1/room/view" id="navbarDropdown2" role="button" aria-haspopup="true" aria-expanded="false">Listings </a>
                                {#<div class="dropdown-menu" aria-labelledby="navbarDropdown2">#}
                                    {#<a class="dropdown-item" href="/">Home</a>#}
                                    {#<a class="dropdown-item" href="explore.twig">Explore</a>#}
                                    {#<a class="dropdown-item" href="/v1/room/view">Listing</a>#}
                                    {#<a class="dropdown-item" href="single-listing.twig">Single Listing</a>#}
                                    {#<a class="dropdown-item" href="addListing.twig">Contact</a>#}
                                {#</div>#}
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#">Contact</a>
                            </li>
                        </ul>
                        <!-- Search btn -->
                        <div class="dorne-search-btn">
                            <a id="search-btn" href="#"><i class="fa fa-search" aria-hidden="true"></i> Search</a>
                        </div>
                        <!-- Signin btn -->
                        <div class="dorne-signin-btn">
                            <a rel="stylesheet" type="text/css" href="/login">Sign in or Register</a>
                        </div>
                        <!-- Add listings btn ../../app/controller/Admin.php-->
                        <div class="dorne-add-listings-btn">
                            <a href="/addListing" class="btn dorne-btn">+ Add Listings</a>
                        </div>
                    </div>
                </nav>
            </div>
        </div>
    </div>
</header>
<!-- ***** Header Area End ***** -->

<!-- ***** Welcome Area Start ***** -->
<section class="dorne-welcome-area bg-img bg-overlay" style="background-image: url(/res/img/bg-img/hero-1.jpg);">
    <div class="container h-100">
        <div class="row h-100 align-items-center justify-content-center">
            <div class="col-12 col-md-10">
                <!-- Hero Search Form -->
                <div class="hero-search-form">
                    <!-- Tabs -->
                    <div class="nav nav-tabs" id="heroTab" role="tablist">
                        <a class="nav-item nav-link active" id="nav-places-tab" data-toggle="tab" href="#nav-places" role="tab" aria-controls="nav-places" aria-selected="true">Members</a>
                        <a class="nav-item nav-link" id="nav-events-tab" data-toggle="tab" href="#nav-events" role="tab" aria-controls="nav-events" aria-selected="false">Admins</a>
                    </div>
                    <!-- Tabs Content -->
                    <div class="tab-content" id="nav-tabContent">
                        <div class="tab-pane fade show active" id="nav-places" role="tabpanel" aria-labelledby="nav-places-tab">
                            <h6>MEMBERS LIST</h6>
                            {#<div class="list">#}
                                {#<form action="#" method="get">#}
                                    <div class="listing-menu-area mt-100" id="menu">
                                        {% for i in range(0,i-1) %}
                                        <div class="single-listing-menu d-flex justify-content-between">
                                            <!-- Listing Menu Title -->
                                            <div class="listing-menu-title">
                                                <h6>User Name: {{ members[i].first_name }} {{ members[i].last_name }}<br/></h6>
                                                <p>User Email: {{ members[i].email }}<br/><br/></p>
                                            </div>
                                            <!-- Listing Menu Price -->
                                            {#<div class="listing-menu-price">#}
                                                {#<h6>$9,90</h6>#}
                                            {#</div>#}
                                        </div>
                                        {% endfor %}
                                    </div>



                                {#</form>#}
                            {#</div>#}
                        </div>

                        <div class="tab-pane fade" id="nav-events" role="tabpanel" aria-labelledby="nav-events-tab">
                           <h6>ADMINS LIST</h6>
                                {#<form action="#" method="get">#}
                                    <div class="listing-menu-area mt-100" id="menu">
                                    {% for i in range(0,1) %}
                                        <div class="single-listing-menu d-flex justify-content-between">
                                            <!-- Listing Menu Title -->
                                            <div class="listing-menu-title">
                                        Name : {{ admin[i].name }}  <br/> Email address: {{ admin[i].email }}<br/>
                                            </div>
                                        </div>
                                    {% endfor %}
                                    </div>
                                {#</form>#}
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- jQuery-2.2.4 js -->
<script src="/res/js/jquery/jquery-2.2.4.min.js"></script>
<!-- Popper js -->
<script src="/res/js/bootstrap/popper.min.js"></script>
<!-- Bootstrap-4 js -->
<script src="/res/js/bootstrap/bootstrap.min.js"></script>
<!-- All Plugins js -->
<script src="/res/js/others/plugins.js"></script>
<!-- Active JS -->
<script src="/res/js/active.js"></script>
</body>

</html>